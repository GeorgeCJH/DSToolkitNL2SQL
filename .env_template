# Environment variables template for DSToolkit NL2SQL
# Copy this file to .env and fill in your actual values

# Azure Identity
IdentityType=user_assigned
AZURE_CLIENT_ID=your-client-id-here

# Database Configuration
Text2Sql__DatabaseEngine=postgres
Text2Sql__UseQueryCache=False
Text2Sql__PreRunQueryCache=False
Text2Sql__UseColumnValueStore=True
Text2Sql__GenerateFollowUpSuggestions=True
Text2Sql__RowLimit=100

# Azure OpenAI Configuration
OpenAI__CompletionModel=gpt-4o
OpenAI__CompletionDeployment=your-completion-deployment
OpenAI__MiniCompletionModel=gpt-4o-mini
OpenAI__MiniCompletionDeployment=your-mini-completion-deployment
OpenAI__Endpoint=https://your-openai-endpoint.openai.azure.com/
OpenAI__ApiVersion=2025-01-01-preview
OpenAI__EmbeddingModel=text-embedding-ada-002
OpenAI__EmbeddingDeployment=your-embedding-deployment
OpenAI__EmbeddingDimensions=1536

# Azure AI Search Configuration
AIService__AzureSearchOptions__Endpoint=https://your-search-service.search.windows.net
AIService__AzureSearchOptions__Text2SqlSchemaStore__Index=nl2sql-schema-store-index
AIService__AzureSearchOptions__Text2SqlSchemaStore__SemanticConfig=nl2sql-schema-store-semantic-config
AIService__AzureSearchOptions__Text2SqlQueryCache__Index=nl2sql-query-cache-index
AIService__AzureSearchOptions__Text2SqlQueryCache__SemanticConfig=nl2sql-query-cache-semantic-config
AIService__AzureSearchOptions__Text2SqlColumnValueStore__Index=nl2sql-column-value-store-index

# Storage Account Configuration
StorageAccount__ConnectionString=DefaultEndpointsProtocol=https;AccountName=your-storage-account;AccountKey=your-storage-key;EndpointSuffix=core.windows.net
StorageAccount__Nl2SqlSchemaStore__Container=nl2sql-schema-store
StorageAccount__Nl2SqlQueryCache__Container=nl2sql-query-cache
StorageAccount__Nl2SqlColumnValueStore__Container=nl2sql-column-value-store

# Function App Configuration
FunctionApp__Endpoint=https://your-function-app.azurewebsites.net
FunctionApp__AppRegistrationResourceId=/subscriptions/your-subscription/resourceGroups/your-rg/providers/Microsoft.Web/sites/your-function-app

# Database Connection (PostgreSQL example)
Text2Sql__Postgres__Database=your-database
Text2Sql__Postgres__User=your-username
Text2Sql__Postgres__Password=your-password
Text2Sql__Postgres__ServerHostname=your-server.postgres.database.azure.com
Text2Sql__Postgres__Port=5432

# Use Case Configuration
NL2SQL_USE_CASE=Query your business data